<?xml version="1.0" encoding="utf-8"?>
<cruise xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="cruise-config.xsd" schemaVersion="89">
  <server artifactsdir="artifacts" agentAutoRegisterKey="123456789abcdef" commandRepositoryLocation="default" serverId="c252898c-10dd-4315-aeb9-5338e88da4bf">
    <security>
      <passwordFile path="/etc/go/go-users" />
      <roles>
        <role name="pauser">
          <users>
            <user>user</user>
          </users>
        </role>
      </roles>
    </security>
  </server>
  <pipelines group="asd">
    <pipeline name="asd-training">
      <materials>
        <git url="https://github.com/caligin/tinytypes.git" />
      </materials>
      <stage name="build">
        <jobs>
          <job name="build">
            <tasks>
              <exec command="mvn">
                <arg>clean</arg>
                <arg>package</arg>
              </exec>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
    <pipeline name="asdasd-sta">
      <materials>
        <svn url="asdasdasd" username="" />
      </materials>
      <stage name="defaultStage">
        <jobs>
          <job name="defaultJob">
            <tasks>
              <ant />
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
  </pipelines>
  <pipelines group="somethingsomethingtrainingside">
    <pipeline name="blah-training" isLocked="false">
      <materials>
        <svn url="asasd/nope" username="" />
      </materials>
      <stage name="defaultStage">
        <jobs>
          <job name="defaultJob">
            <tasks>
              <ant />
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
    <pipeline name="wqdwwqd-prod">
      <materials>
        <svn url="asdasd" username="" />
      </materials>
      <stage name="defaultStage">
        <jobs>
          <job name="defaultJob">
            <tasks>
              <ant />
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
  </pipelines>
  <environments>
    <environment name="training">
      <environmentvariables>
        <variable name="env">
          <value>tra</value>
        </variable>
      </environmentvariables>
      <pipelines>
        <pipeline name="asd-training" />
        <pipeline name="blah-training" />
      </pipelines>
    </environment>
    <environment name="prod">
      <pipelines>
        <pipeline name="wqdwwqd-prod" />
      </pipelines>
    </environment>
    <environment name="staging">
      <pipelines>
        <pipeline name="asdasd-sta" />
      </pipelines>
    </environment>
  </environments>
</cruise>

